<div class="container">
<%=link_to "ユーザー一覧",admin_managers_path %>
<%=link_to "コーディネート登録、編集",new_admin_manager_product_path %>
<table>
	<thead>
		<tr>
			<th>画像</th>
		</tr>
	</thead>
		<% @rains.each do |rain| %>
		<tr>
			<td>
				<%= rain.name %>
				<%= rain.introduction %>
				<%= attachment_image_tag rain, :rain_image, :size =>'300x400' %>
			</td>
			<td><%=link_to "削除します",admin_manager_product_path(rain.id),method: :delete , data:{confirm:  "本当に？"} %>
			</td>
		</tr>
			<% end %>


	</tbody>
</table>
</div>